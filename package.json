    "cors": "2.8.5",
    "dotenv": "16.4.5",
    "email-validator": "2.0.4",
    "express": "4.19.2",
    "helmet": "7.1.0",
    "hex-color-regex": "1.1.0",
    "jsonwebtoken": "9.0.2",
    "prisma": "5.15.0",
    "tsx": "4.15.2"
  },
  "devDependencies": {
    "@types/bcrypt": "5.0.2",
    "@types/body-parser": "1.19.5",
    "@types/cors": "2.8.17",
