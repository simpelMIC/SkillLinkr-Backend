{
  "name": "skillmatch-backend",
  "version": "0.0.0",
  "description": "skillmatch-backend",
  "main": "index.js",
  "scripts": {
    "dev": "npx tsc -w",
    "build": "npx tsc",
    "server": "nodemon ./build/server/index.js",
    "prisma-migrate": "cd src/database/prisma && npx prisma migrate dev --name auto-update",
    "prisma-migrate:create-only": "cd src/database/prisma && npx prisma migrate dev --name auto-update --create-only",
    "setup": "npx tsx ./seedDatabase.ts && npx tsx ./setup.ts",
    "update": "npx npm-check -u"
  },
  "author": "KeineAhnung",
  "license": "",
  "type": "module",
  "dependencies": {
    "@prisma/client": "5.15.0",
